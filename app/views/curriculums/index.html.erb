<h1>Curriculums</h1>

<table>
  <thead>
    <tr>
      <th>Nombre</th>
      <th>Edad</th>
      <th>Sexo</th>
      <th>Direccion</th>
      <th>Codigo postal</th>
      <th>Ciudad</th>
      <th>Email</th>
      <th>Telefono</th>
      <th>Celular</th>
      <th>Especialidad</th>
      <th>Grado estudios</th>
      <th>Sueldo dec</th>
      <th>Herramientas usadas</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @curriculums.each do |curriculum| %>
      <tr>
        <td><%= curriculum.nombre %></td>
        <td><%= curriculum.edad %></td>
        <td><%= curriculum.sexo %></td>
        <td><%= curriculum.direccion %></td>
        <td><%= curriculum.codigo_postal %></td>
        <td><%= curriculum.ciudad %></td>
        <td><%= curriculum.email %></td>
        <td><%= curriculum.telefono %></td>
        <td><%= curriculum.celular %></td>
        <td><%= curriculum.especialidad %></td>
        <td><%= curriculum.grado_estudios %></td>
        <td><%= curriculum.sueldo_dec %></td>
        <td><%= curriculum.herramientas_usadas %></td>
        <td><%= link_to 'Show', curriculum %></td>
        <td><%= link_to 'Edit', edit_curriculum_path(curriculum) %></td>
        <td><%= link_to 'Destroy', curriculum, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>
